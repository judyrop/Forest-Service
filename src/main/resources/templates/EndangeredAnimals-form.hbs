{{#partial "content"}}
<div class="container">
<form action="/EndangeredAnimals-success"method= "post"class="endangered">
    <div class="form-control">
        <label for="name">Name</label>
        <input id="name" placeholder="Name of endangeredAnimals" name="name" type="text" value="">
    </div>
    <div class="form-control">

        <label for="age">Age</label>
        <select name="age" id="age" class="form-group">
            <option value="newborn">newborn</option>
            <option value="young">young</option>
            <option value="adult">adult</option>

        </select>
    </div>
    <div class="form-control">
        <label for="health">Health</label>
        <select name="health" id="health" class="form-group">
            <option value="healthy">healthy</option>
            <option value="ill">ill</option>
            <option value="okay">okay</option>

        </select>
    </div>
    <div class="form-group">
                        <label for="location">Location</label>
                      <select name="location" id="Location" class="form-group">
                           <option value="newborn">Zone A</option>
                          <option value="young">Near the River</option>
                         <option value="adult">NE Quadrant</option>
                         <option value="adult">SE Quadrant</option>
                         <option value="adult">Jungle A</option>
                     </select>
                    </div>
                <div class="form-group">
                        <label for="rangerName">Ranger Name</label>
                        <select name="location" id="Location" class="form-group">
                             <option value="rangerName">James Bill</option>
                             <option value="rangerName">Juanita Lions</option>
                              <option value="rangerName">Heavi  Kins</option>
                               <option value="rangerName">Joels  Tori</option>
                              <option value="rangerName">Barack Zera</option>
                        </select>
                    </div>
    <button type="submit" class="btn btn primary">Submit</button>
    <p><a href="/" class="btn btn success">Back Home </a></p>
</form>

{{/partial}}
</div>
{{> layout.hbs}}